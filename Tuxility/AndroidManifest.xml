<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="com.grimmvarg.android.tuxility" android:installLocation="auto"
	android:versionName="1.0" android:versionCode="1">

	<application android:icon="@drawable/icon" android:label="@string/app_name">
		<uses-sdk android:minSdkVersion="7" android:targetSdkVersion="8"/>
		<activity android:name=".Launch" android:label="@string/app_name"
			android:screenOrientation="portrait">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity android:name=".BackupManagement" android:label="@string/app_name"
			android:screenOrientation="portrait">
		</activity>
		<activity android:name=".KernelHacking" android:label="@string/app_name"
			android:screenOrientation="portrait">
		</activity>
		<activity android:name=".RebootManager" android:label="@string/app_name"
			android:screenOrientation="portrait">
		</activity>
		<activity android:name=".FileChooser" android:label="@string/app_name"
			android:screenOrientation="portrait">
		</activity>
		<activity android:name=".PhoneSettingsAndTweaks"
			android:label="@string/app_name" android:screenOrientation="portrait">
		</activity>
		<activity android:name=".SettingsEditor" android:label="@string/app_name"
			android:screenOrientation="portrait">
		</activity>



	</application>




</manifest> 